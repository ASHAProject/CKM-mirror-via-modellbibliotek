		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Genetisk repeterte sekvensvariant">
					description = <"En genetisk variant hvor et segment med en eller flere nukleotider (det repeterte elementet) blir gjentatt flere ganger, sammenliknet med referansesekvenset. De repeterte elementene kan være satt sammen av alt fra en til veldig mange nukleotider, av og til hele gener. (Stadig gjentagende like nukleotidsekvenser i DNA.)
Også kalt Repetert DNA eller repeterte elementer.">
				>
				["at0001"] = <
					text = <"Startposisjon">
					description = <"Plasseringen/posisjonen til det første nukleotidet i det området som har blitt repetert/gjentatt.">
				>
				["at0003"] = <
					text = <"Sluttposisjonen">
					description = <"Plasseringen/posisjonen til det siste nukleotidet i det området som har blitt repetert/gjentatt.">
				>
				["at0005"] = <
					text = <"Repetert sekvens">
					description = <"Nukleotidesekvensen som har blitt repetert.">
				>
				["at0007"] = <
					text = <"Kopitall">
					description = <"Antall ganger nukleotidesekvensen har blitt repetert.">
				>
				["at0008"] = <
					text = <"Referansesekvens">
					description = <"Sekvensfilen som har blitt brukt som en referanse for å beskrive varianten.">
				>
			>
