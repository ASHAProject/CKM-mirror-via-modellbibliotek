definition
	CLUSTER[at0000] matches {    -- ART container details
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..*} matches {    -- Embryo ID
				value matches {
					DV_IDENTIFIER matches {*}
				}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- Freeze date
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Freeze method
				value matches {
					DV_TEXT matches {*}
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0004,    -- Slow freeze
							at0005]    -- Vitrification
						}
					}
				}
			}
			ELEMENT[at0006] occurrences matches {0..1} matches {    -- Freeze protocol ID
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Freeze development day
				value matches {
					DV_COUNT matches {*}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {    -- Thaw date
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0009] occurrences matches {0..1} matches {    -- Thaw reason
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0010] occurrences matches {0..1} matches {    -- Expiry date
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0011] occurrences matches {0..1} matches {    -- Destruction date
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0012] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"ART container details">
					description = <"Details about the contents of a container stored for the purposes of assisted reproduction treatment.">
				>
				["at0001"] = <
					text = <"Embryo ID">
					description = <"The unique identifier for the oocyte or embryo.">
					comment = <"This data element has multiple occurrences to allow for more than one embryo per container.">
				>
				["at0002"] = <
					text = <"Freeze date">
					description = <"The date and/or time when the container and its' contents were frozen.">
				>
				["at0003"] = <
					text = <"Freeze method">
					description = <"The method of freezing.">
				>
				["at0004"] = <
					text = <"Slow freeze">
					description = <"">
				>
				["at0005"] = <
					text = <"Vitrification">
					description = <"">
				>
				["at0006"] = <
					text = <"Freeze protocol ID">
					description = <"The identifier for the freeze procedure.">
				>
				["at0007"] = <
					text = <"Freeze development day">
					description = <"The post-insemination day on which the embryo was frozen.">
				>
				["at0008"] = <
					text = <"Thaw date">
					description = <"The date and/or time when the container and its' contents were thawed.">
				>
				["at0009"] = <
					text = <"Thaw reason">
					description = <"The reason for thawing the contents of the container.">
				>
				["at0010"] = <
					text = <"Expiry date">
					description = <"The date beyond the contents are not to be used.">
				>
				["at0011"] = <
					text = <"Destruction date">
					description = <"The date on which the contents awere destroyed.">
				>
				["at0012"] = <
					text = <"Comment">
					description = <"Additional narrative about the ART straw contents, not captured in other fields.">
				>
			>
		>
	>
