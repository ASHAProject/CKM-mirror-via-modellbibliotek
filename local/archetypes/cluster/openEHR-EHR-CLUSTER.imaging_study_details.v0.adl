definition
	CLUSTER[at0000] matches {    -- Imaging study details
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {1..*} matches {    -- Study identifier
				value matches {
					DV_IDENTIFIER matches {*}
				}
			}
			ELEMENT[at0002] matches {    -- Study status
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0003,    -- Registered
							at0004,    -- Available
							at0005,    -- Cancelled
							at0006]    -- Unknown
						}
					}
				}
			}
			ELEMENT[at0014] occurrences matches {0..1} matches {    -- Study description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..*} matches {    -- Modality
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {    -- Study date
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0009] occurrences matches {0..*} matches {    -- Study end point
				value matches {
					DV_URI matches {*}
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0012] occurrences matches {0..1} matches {    -- Procedure reason
				value matches {
					DV_TEXT matches {*}
				}
			}
			CLUSTER[at0015] occurrences matches {0..*} matches {    -- Imaging series details
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0016] matches {    -- Series identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {    -- Series number
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0018] occurrences matches {0..*} matches {    -- Modality
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {    -- Number of instances
						value matches {
							DV_COUNT matches {*}
						}
					}
					ELEMENT[at0019] occurrences matches {0..1} matches {    -- Series end point
						value matches {
							DV_URI matches {*}
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0021] occurrences matches {0..*} matches {    -- Device
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
			ELEMENT[at0013] occurrences matches {0..1} matches {    -- Technique
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0011] occurrences matches {0..*} matches {    -- Procedure
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0020] occurrences matches {0..*} matches {    -- Device
				include
					archetype_id/value matches {/.*/}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Imaging study details">
					description = <"Details about specific images, including DICOM studies, series or instances.">
				>
				["at0001"] = <
					text = <"Study identifier">
					description = <"Unique identifier for the imaging study assigned by the radiology service.">
					comment = <"Also known in FHIR as ImagingStudy.identifier.">
				>
				["at0002"] = <
					text = <"Study status">
					description = <"The current state of the Study.">
					comment = <"Also known in FHIR as ImagingStudy.status.">
				>
				["at0003"] = <
					text = <"Registered">
					description = <"The existence of the imaging study is registered, but there is nothing yet available.">
				>
				["at0004"] = <
					text = <"Available">
					description = <"At least one instance has been associated with this imaging study.">
				>
				["at0005"] = <
					text = <"Cancelled">
					description = <"The imaging study is unavailable because the study was not started or not completed.">
				>
				["at0006"] = <
					text = <"Unknown">
					description = <"The status is not known by the radiology source system.">
				>
				["at0007"] = <
					text = <"Modality">
					description = <"The type of diagnostic equipment, or function or technique of that equipment, that originally acquired, the data used to create the image study.">
					comment = <"Coding of the 'Modality' with a value set is recommended. For example, the DICOM CID 29 Acquisition Modality value set. This data element has multiple occurrences to allow specification of submodalities such as 'Doppler' as a form of 'Ultrasound'. Also known in FHIR as ImagingStudy.modality.">
				>
				["at0008"] = <
					text = <"Study date">
					description = <"Date and time the study started.">
					comment = <"Also known in FHIR as ImagingStudy.started.">
				>
				["at0009"] = <
					text = <"Study end point">
					description = <"Location of where to query for imaging content and metadata (QIDO-RS, WADO-RS, or WADO-URI).">
					comment = <"The network service providing access (e.g., query, view, or retrieval) for the study. Also known in FHIR as ImagingStudy.endpoint.">
				>
				["at0010"] = <
					text = <"Number of instances">
					description = <"The number of instances contained in the series.">
					comment = <"Also known in FHIR as ImagingStudy.series.numberOfInstances.">
				>
				["at0011"] = <
					text = <"Procedure">
					description = <"Identification of the clinical procedure or management used as part of capturing the study.">
					comment = <"Coding of the 'Procedure ' with a terminology is recommended. In implementations, RM links can be created from this data element to relevant ACTION.medication, ACTION.procedure or similar archetypes stored within the health record that document the actual clinical procedures and related activities carried out. Multiple occurrences allow for identification of one or more techniques. If the procedure needs to be recorded at the whole of result level, use the 'Procedure' data element within the OBSERVATION.imaging_exam_result archetype instead.">
				>
				["at0012"] = <
					text = <"Procedure reason">
					description = <"">
				>
				["at0013"] = <
					text = <"Technique">
					description = <"">
					comment = <"Coding of the 'Technique' with a terminology is recommended. Multiple occurrences allow for identification of one or more techniques. If the technique needs to be recorded at the whole of result level, use the 'Technique' data element within the OBSERVATION.imaging_exam_result archetype instead.">
				>
				["at0014"] = <
					text = <"Study description">
					description = <"Institution-generated description or classification of the study (component) performed.">
					comment = <"Also known in FHIR as ImagingStudy.description and DICOM as 'Study description'.">
				>
				["at0015"] = <
					text = <"Imaging series details">
					description = <"">
				>
				["at0016"] = <
					text = <"Series identifier">
					description = <"Unique identifier for the imaging study assigned by the radiology service.">
					comment = <"Also known in FHIR as ImagingStudy.identifier.">
				>
				["at0017"] = <
					text = <"Series number">
					description = <"The numeric identifier of this series in the study.">
					comment = <"Also known in FHIR as ImagingStudy.series.number.">
				>
				["at0018"] = <
					text = <"Modality">
					description = <"The type of diagnostic equipment, or function or technique of that equipment, that originally acquired, the data used to create the image series.">
					comment = <"Coding of the 'Modality' with a value set is recommended. For example, the DICOM CID 29 Acquisition Modality value set. This data element has multiple occurrences to allow specification of submodalities such as 'Doppler' as a form of 'Ultrasound'. Also known in FHIR as ImagingStudy.series.modality.">
				>
				["at0019"] = <
					text = <"Series end point">
					description = <"Location of where to query for imaging content and metadata (QIDO-RS, WADO-RS, or WADO-URI).">
					comment = <"The network service providing access (e.g., query, view, or retrieval) for the series. Also known in FHIR as ImagingStudy.series.endpoint.">
				>
				["at0020"] = <
					text = <"Device">
					description = <"Details about imaging device/s used to capture the study.">
				>
				["at0021"] = <
					text = <"Device">
					description = <"Details about imaging device/s used to capture the series.">
				>
			>
		>
	>
