definition
	CLUSTER[at0000] matches {    -- Boston Bowel Preparation Scale
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..1} matches {    -- Right colon
				value matches {
					3|[local::at0005],
					2|[local::at0004],
					1|[local::at0003],
					0|[local::at0002]
				}
			}
			ELEMENT[at0013] occurrences matches {0..1} matches {    -- Transverse
				value matches {
					3|[local::at0014],
					2|[local::at0015],
					1|[local::at0016],
					0|[local::at0017]
				}
			}
			ELEMENT[at0018] occurrences matches {0..1} matches {    -- Left colon
				value matches {
					3|[local::at0019],
					2|[local::at0020],
					1|[local::at0021],
					0|[local::at0022]
				}
			}
			ELEMENT[at0012] occurrences matches {0..1} matches {    -- Total score
				value matches {
					DV_COUNT matches {
						magnitude matches {|<=9|}
					}
				}
			}
			ELEMENT[at0008] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}



