definition
	CLUSTER[at0000] matches {	-- Relative anatomical location
		items cardinality matches {1..*; unordered} matches {
			CLUSTER[at0020] occurrences matches {0..*} matches {	-- Relative location
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0021] matches {	-- Identified landmark
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- Distance from landmark
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::122]>
								list = <
									["1"] = <
										units = <"cm">
										magnitude = <|>=0.0|>
										precision = <|1|>
									>
									["2"] = <
										units = <"in">
										magnitude = <|>=0.0|>
										precision = <|1|>
									>
									["3"] = <
										units = <"mm">
										magnitude = <|>=0.0|>
										precision = <|0|>
									>
								>
							>
						}
					}
					ELEMENT[at0006] occurrences matches {0..2} matches {	-- Aspect
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0007, 	-- Medial to
									at0008, 	-- Lateral to
									at0009, 	-- Superior to
									at0010, 	-- Inferior to
									at0012, 	-- Anterior to
									at0013, 	-- Posterior to
									at0055, 	-- Proximal to
									at0056, 	-- Distal to
									at0057, 	-- Superficial to
									at0058, 	-- Deep to
									at0059, 	-- Within
									at0060]	-- External to
								}
							}
							DV_TEXT matches {*}
						}
					}
				}
			}
			ELEMENT[at0023] occurrences matches {0..*} matches {	-- Description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0053] occurrences matches {0..*} matches {	-- Visual markings/orientation
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0054] occurrences matches {0..*} matches {	-- Multimedia representation
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1/}
			}
		}
	}


