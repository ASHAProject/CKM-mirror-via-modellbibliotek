definition
	OBSERVATION[at0000] matches {    -- Imaging examination result
		data matches {
			HISTORY[at0001] matches {    -- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0004] matches {    -- Test name
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0055] occurrences matches {0..1} matches {    -- Target body site
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0006] occurrences matches {0..*} matches {    -- Structured target body site
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0070] occurrences matches {0..1} matches {    -- Examination date
										value matches {
											DV_DATE_TIME matches {*}
											DV_INTERVAL<DV_DATE_TIME> matches {*}
										}
									}
									ELEMENT[at0072] occurrences matches {0..*} matches {    -- Overall test status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0073,    -- Registered
													at0074,    -- Partial
													at0075,    -- Preliminary
													at0076,    -- Final
													at0077,    -- Amended
													at0078,    -- Corrected
													at0079,    -- Appended
													at0080,    -- Cancelled
													at0081,    -- Entered in error
													at0090]    -- Unknown
												}
											}
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0071] occurrences matches {0..1} matches {    -- Overall test status timestamp
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {    -- DateTime result issued
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0061] occurrences matches {0..1} matches {    -- Reason for request
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {    -- Clinical information provided
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {    -- Imaging findings
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0044] occurrences matches {0..*} matches {    -- Structured imaging findings
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_exam(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.imaging_finding(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.imaging_finding(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.imaging_finding\.v1|openEHR-EHR-CLUSTER\.imaging_finding(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.imaging_exam\.v1|openEHR-EHR-CLUSTER\.imaging_exam(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0056] occurrences matches {0..1} matches {    -- Comparison
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0063] occurrences matches {0..1} matches {    -- Imaging quality
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0057] occurrences matches {0..1} matches {    -- Imaging quality description
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {    -- Overall impression
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0058] occurrences matches {0..*} matches {    -- Imaging differential diagnosis
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0020] occurrences matches {0..*} matches {    -- Imaging diagnosis
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0059] occurrences matches {0..*} matches {    -- Recommendation
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0045] occurrences matches {0..*} matches {    -- Report representation
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.media_file(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0023] occurrences matches {0..*} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0047] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0048] occurrences matches {0..1} matches {    -- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0025] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0026] occurrences matches {0..*} matches {    -- Imaging service
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0033] occurrences matches {0..1} matches {    -- Report identifier
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					CLUSTER[at0027] occurrences matches {0..*} matches {    -- Examination request details
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0031] occurrences matches {0..1} matches {    -- Receiver order identifier
								value matches {
									DV_IDENTIFIER matches {*}
									DV_CODED_TEXT matches {*}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {    -- Requester order identifier
								value matches {
									DV_IDENTIFIER matches {*}
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0029] occurrences matches {0..*} matches {    -- Examination requested name
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0030] occurrences matches {0..*} matches {    -- Requester
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					allow_archetype CLUSTER[at0083] occurrences matches {0..1} matches {    -- Distribution list
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.organisation(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v0|openEHR-EHR-CLUSTER\.person(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0087] occurrences matches {0..*} matches {    -- Technique
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0049] occurrences matches {0..*} matches {    -- Technique description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0088] occurrences matches {0..*} matches {    -- Procedure
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0062] occurrences matches {0..*} matches {    -- Procedure description
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0041] occurrences matches {0..*} matches {    -- Structured technique/procedure
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0065] occurrences matches {0..*} matches {    -- Study details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_study_details\.v0/}
					}
					allow_archetype CLUSTER[at0067] occurrences matches {0..*} matches {    -- Comparison image/s
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.imaging_study_details(-[a-zA-Z0-9_]+)*\.v0/}
					}
					allow_archetype CLUSTER[at0069] occurrences matches {0..*} matches {    -- Device
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype CLUSTER[at0046] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Imaging examination result">
					description = <"Record the findings and interpretation of an imaging examination performed.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Any event">
					description = <"Default, unspecified point in time or interval event which may be explicitly defined in a template or at run-time.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Test name">
					description = <"The name of the imaging examination or procedure performed.">
					comment = <"Coding with a terminology is strongly recommended, preferably a term specifying both modality and anatomical location, where possible. Possible candidate terminologies are LOINC, SNOMED CT or RadLex.">
				>
				["at0006"] = <
					text = <"Structured target body site">
					description = <"Structured detail about the anatomical site related to the entire result.">
					comment = <"If the 'Target site' is specified by a code in the 'Test name', then this field may be redundant. Identification of an anatomical site related to a specific finding will be recorded within each instance of CLUSTER.imaging_finding.">
				>
				["at0008"] = <
					text = <"Imaging findings">
					description = <"Narrative description of the clinical findings.">
				>
				["at0014"] = <
					text = <"Clinical information provided">
					description = <"Narrative description of relevant clinical history that provides context for the examination and interpretation of results. Also known as 'Clinical summary'.">
					comment = <"This may include a link to the clinical information provided in the original examination request or elsewhere in the health record.">
				>
				["at0020"] = <
					text = <"Imaging diagnosis">
					description = <"Single word, phrase or brief description representing the likely condition or diagnosis.">
					comment = <"This data element has multiple occurrences to allow for more than one diagnoses. Coding with a terminology is preferred, where possible. This data element should be regarded as mutually exclusive to 'Differential diagnoses' - only one of 'Differential diagnoses' OR 'Imaging diagnosis' should be present in the each Imaging examination result.">
				>
				["at0021"] = <
					text = <"Overall impression">
					description = <"Narrative concise, clinically relevant interpretation of all imaging findings, and include a comparison with previous studies where appropriate.">
					comment = <"Also referred to as 'Opinion' or 'Conclusion'. Usually equivalent to the FHIR DiagnosticReport.conclusion.">
				>
				["at0023"] = <
					text = <"Comment">
					description = <"Additional narrative about the examination not captured in other fields.">
					comment = <"For example: a note that the film was given to the patient.">
				>
				["at0024"] = <
					text = <"DateTime result issued">
					description = <"The date and/or time that the result was issued for the recorded 'Overall test status'.">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Imaging service">
					description = <"Demographic details about the service, organisation or individual carrying out the imaging examination.">
				>
				["at0027"] = <
					text = <"Examination request details">
					description = <"Details concerning a single examination requested.">
					comment = <"Usually there is one examination request for each result, however in some circumstances multiple examination requests may be represented using a single Imaging examination result archetype.">
				>
				["at0028"] = <
					text = <"Requester order identifier">
					description = <"The local identifier assigned to the order by the order requester.">
					comment = <"Equivalent to the HL7 Placer Order Identifier.">
				>
				["at0029"] = <
					text = <"Examination requested name">
					description = <"Identification of imaging examination or procedure requested, where the examination requested differs from the examination actually performed.">
				>
				["at0030"] = <
					text = <"Requester">
					description = <"Details about the clinician or organisation requesting the imaging examination.">
				>
				["at0031"] = <
					text = <"Receiver order identifier">
					description = <"The local identifier assigned to the imaging examination order by the order filler, usually by the Radiology Information System (RIS).">
					comment = <"Assigning an identifier to a request by the Radiology lnformation System (RIS) enables tracking progress of the request and enables linking results to requests. It also provides a reference to assist with enquiries and it is usually equivalent to the HL7 Filler Order Identifier.">
				>
				["at0033"] = <
					text = <"Report identifier">
					description = <"The unique identifier designated by the imaging service to the report.">
				>
				["at0041"] = <
					text = <"Structured technique/procedure">
					description = <"Additional structured details of technical and clinical aspects of capturing the image/s.">
				>
				["at0044"] = <
					text = <"Structured imaging findings">
					description = <"Structured details about the imaging examination findings targeting specific structure or region.">
				>
				["at0045"] = <
					text = <"Report representation">
					description = <"Digital representation of the examination result.">
					comment = <"Multiple formats are allowed but they should represent equivalent clinical content. For example a pdf version of the report. ">
				>
				["at0046"] = <
					text = <"Extension">
					description = <"Additional information required to extend the model with local content or to align with other reference models or formalisms.">
					comment = <"For example: local information requirements; or additional metadata to align with FHIR.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Confounding factors">
					description = <"Narrative description of factors, not recorded elsewhere, that may influence the examination findings and/or result.">
				>
				["at0049"] = <
					text = <"Technique description">
					description = <"Narrative description about the technique or technical procedure used to capture the image/s.">
					comment = <"Use this data element to provide an outline of technique, including technical limitations or deviation from the normal technique. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data.">
				>
				["at0055"] = <
					text = <"Target body site">
					description = <"Simple description about the body site or region targetted for imaging.">
					comment = <"If the 'Target site' is specified by a code in the 'Test name', then this field may be redundant. Usually equivalent to the FHIR ImagingStudy.series.bodySite">
				>
				["at0056"] = <
					text = <"Comparison">
					description = <"Narrative description about the comparison of this examination, with previous similar studies.">
					comment = <"Also use this data element to indicate if no comparison images and/or reports are available. ">
				>
				["at0057"] = <
					text = <"Imaging quality description">
					description = <"Narrative description about the quality of the examination.">
					comment = <"For example: the nature of any limitations and their impact on interpretation; recommendation that the examination should be repeated; or whether an alternative method of assessment might be more appropriate.">
				>
				["at0058"] = <
					text = <"Imaging differential diagnosis">
					description = <"Single word, phrase or brief description representing a possible condition or diagnosis.">
					comment = <"This data element has multiple occurrences to allow for more than one differential diagnoses. Coding with a terminology is preferred, where possible. This data element should be regarded as mutually exclusive to 'Imaging diagnosis' - only one of 'Differential diagnoses' OR 'Imaging diagnosis' should be present in each Imaging examination result.">
				>
				["at0059"] = <
					text = <"Recommendation">
					description = <"Suggestion for further imaging, investigations and/or referral, and associated rationale.">
					comment = <"This data element has 0..* occurrences to allow for more than one recommendation. Formal orders for additional imaging examination, investigation should be recorded using an INSTRUCTION archetype, such as INSTRUCTION.service_request.">
				>
				["at0061"] = <
					text = <"Reason for request">
					description = <"Narrative description about the reason for requesting the examination. Also known as 'Clinical query'.">
				>
				["at0062"] = <
					text = <"Procedure description">
					description = <"Narrative description about the clinical procedure or other clinical considerations used to capture the image/s.">
					comment = <"Use this data element to describe a summary of the clinical procedure, patient positioning, interventions such as Valsalva manoeuvre, and administration of contrast agent, radiopharmaceuticals and/or treatments as part of the imaging study. For example: '80 ml of Isovue-370 was administered intravenously using a bolus-tracking technique centered on the main pulmonary artery'. It can also be used to record an overview of any issues, complications or adverse reactions encountered. ">
				>
				["at0063"] = <
					text = <"Imaging quality">
					description = <"Assessment about the quality of the examination.">
					comment = <"Coding of the imaging quality with a value set is recommended. For example: 'excellent, good, fair, poor, and non-diagnostic'; or 'adequate, suboptimal and inadequate'.
">
				>
				["at0065"] = <
					text = <"Study details">
					description = <"Details about each image being reported.">
				>
				["at0067"] = <
					text = <"Comparison image/s">
					description = <"Details about an image being compared to the reported image/s.">
				>
				["at0069"] = <
					text = <"Device">
					description = <"Details about imaging device/s used to capture the images.">
				>
				["at0070"] = <
					text = <"Examination date">
					description = <"The date and time or interval of data and time when the imaging examination was carried out.">
				>
				["at0071"] = <
					text = <"Overall test status timestamp">
					description = <"The date and/or time that ‘Overall test status’ was assigned.">
				>
				["at0072"] = <
					text = <"Overall test status">
					description = <"The status of the imaging examination result as a whole.">
					comment = <"The values have been specifically chosen to match those in the HL7 FHIR Diagnostic report, historically derived from HL7v2 practice. Other local codes/terms can be used via the Text 'choice'.

This element is multiple occurrence to cater for the use cases where statuses for different aspects of the result have been split into several elements.">
				>
				["at0073"] = <
					text = <"Registered">
					description = <"The existence of the test is registered in the radiology source system, but no result is yet available.">
				>
				["at0074"] = <
					text = <"Partial">
					description = <"This is a partial (e.g. initial, interim or preliminary) result but data may be incomplete or unverified.">
				>
				["at0075"] = <
					text = <"Preliminary">
					description = <"Verified early results are available, but not all results are final. This is a sub-category of 'Partial'.">
				>
				["at0076"] = <
					text = <"Final">
					description = <"The result is complete and verified by an authorised person.">
				>
				["at0077"] = <
					text = <"Amended">
					description = <"Subsequent to being final, the result has been modified.">
				>
				["at0078"] = <
					text = <"Corrected">
					description = <"Subsequent to being final, the result has been modified to correct an error. This is a sub-category of 'Amended'.">
				>
				["at0079"] = <
					text = <"Appended">
					description = <"Subsequent to being final, the result has been modified by adding new content. The existing content is unchanged. This is a sub-category of 'Amended'.">
				>
				["at0080"] = <
					text = <"Cancelled">
					description = <"The result is unavailable because the test was not started or not completed (also sometimes called 'aborted').">
				>
				["at0081"] = <
					text = <"Entered in error">
					description = <"The result has been withdrawn following previously been issued as final.">
				>
				["at0083"] = <
					text = <"Distribution list">
					description = <"Details of additional clinicians or organisations who require a copy of the test result.">
					comment = <"The 'Distribution list' is for information-only, and that the primary recipient of the report is the person intended to act on the information.">
				>
				["at0087"] = <
					text = <"Technique">
					description = <"Name of the technique or technical procedure used to capture the image/s.">
					comment = <"Coding of the 'Technique' with a terminology is pemitted. Multiple occurrences allow for identification of one or more techniques. If the technique needs to be recorded at study level, use the 'Technique' data element within the CLUSTER.imaging_study_detail archetype.">
				>
				["at0088"] = <
					text = <"Procedure">
					description = <"Identification of the clinical procedure or management used as part of capturing the image/s.">
					comment = <"In implementations, RM links can be created from this data element to relevant ACTION.medication, ACTION.procedure or similar archetypes stored within the health record that document the actual clinical procedures and related activities carried out. If the procedure needs to be recorded at study level, use the 'Procedure' data element within the CLUSTER.imaging_study_details archetype instead.">
				>
				["at0090"] = <
					text = <"Unknown">
					description = <"The status of the result is not known.">
				>
			>
		>
		["nb"] = <
			items = <
				["at0000"] = <
					text = <"Bildediagnostisk svar">
					description = <"Registrering av funn og tolkning ved en bildediagnostisk undersøkelse.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Uspesifisert hendelse">
					description = <"Standard, uspesifisert tidspunkt eller tidsintervall som kan defineres mer eksplisitt i et templat eller i en applikasjon.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Bildediagnostisk undersøkelse">
					description = <"Navnet på den utførte bildediagnostiske undersøkelsen eller prosedyren.">
					comment = <"Det anbefales sterkt å bruke en terminologi der det er mulig, fortrinnsvis en kode som både spesifiserer modalitet og anatomisk lokalisering. Mulige terminologier er NCRP, LOINC, SNOMED CT eller RadLex.">
				>
				["at0006"] = <
					text = <"*Structured target body site (en)">
					description = <"Strukturert detaljer om det anatomiske området og som er knyttet til hele resultatet.">
					comment = <"Hvis det anatomiske området er tilstrekkelig spesifisert med en kode i undersøkelsesnavnet, er ikke dette feltet nødvendig. Identifisering av det anatomiske stedet som er knyttet til et spesifikt funn skal registreres i hver enkelt instans av CLUSTER.imaging_finding.">
				>
				["at0008"] = <
					text = <"Funn">
					description = <"Tekstlig beskrivelse av de kliniske funnene.">
				>
				["at0014"] = <
					text = <"Tilgjengelig klinisk informasjon">
					description = <"Beskrivelse av relevant klinisk informasjon som er tilgjengelig på tolkningstidspunktet. Også kalt klinisk sammendrag.">
					comment = <"Dette kan inkludere en lenke til klinisk informasjon som er gitt i den originale henvisningen eller annet sted i den elektroniske pasientjournalen.">
				>
				["at0020"] = <
					text = <"Bildediagnostisk diagnose">
					description = <"Enkeltord, setning eller kort beskrivelse som uttrykker en den sannsynlige tilstand eller diagnose.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn en diagnose. Det er anbefalt å bruke en terminologi dersom mulig. Dataelementet bør være gjensidig utelukkende for \"Bildediagnostisk diffensialdiagnose\", bare ENTEN \"Bildediagnostisk differensialdiagnose\" ELLER \"Bildediagnostisk diagnose\" bør forekomme i hvert Bildediagnostisk svar.">
				>
				["at0021"] = <
					text = <"Konklusjon">
					description = <"Tekstlig presis og klinisk relevant tolkning av alle funn ved bildeundersøkelsen og inkluderer en sammenligning med tidligere granskninger dersom de er passende.">
					comment = <"*Also referred to as 'Opinion' or 'Conclusion'. Usually equivalent to the FHIR DiagnosticReport.conclusion. (en)
Også referert til som \" Vanligvis tilsvarerende FHIR DiagnosticReport.conclusion.">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Ytterligere kommentarer om undersøkelsen som ikke passer i andre felter.">
					comment = <"For eksempel et notat om at kopi av bilde eller svarrapport er gitt til pasienten.">
				>
				["at0024"] = <
					text = <"Dato og tidspunkt for ferdigstilt resultat">
					description = <"*The date and/or time that the result was issued for the recorded 'Overall test status'. (en)">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Tjenesteleverandør">
					description = <"Demografiske opplysninger om den organisasjon eller person som utfører den bildediagnostiske undersøkelsen.">
				>
				["at0027"] = <
					text = <"Henvisningsdetaljer">
					description = <"Detaljer om en enkelt henvisning.">
					comment = <"Vanligvis er det en henvisning per resultat. Imidlertid kan det i noen tilfeller være flere henvisninger som kan lede til at svar blir representert i en enkelt Bildediagnostisk svar-arketype.">
				>
				["at0028"] = <
					text = <"Rekvirentens rekvisisjonsID">
					description = <"Rekvirentens/bestillers lokale ID for rekvisisjonen.">
					comment = <"Tilsvarende HL7 \"Placer Order Identifier\".">
				>
				["at0029"] = <
					text = <"Undersøkelsesnavn">
					description = <"Navn på bildeundersøkelse eller rekvirert prosedyre, når den rekvirerte undersøkelse avviker fra den undersøkelse som faktisk er utført.">
				>
				["at0030"] = <
					text = <"Rekvirent">
					description = <"Detaljer om den kliniker eller organisasjon som rekvirerer undersøkelsen.">
				>
				["at0031"] = <
					text = <"Rekvisisjonsmottakers rekvisisjonsID">
					description = <"*The local identifier assigned to the imaging examination order by the order filler, usually by the receiving Radiology Information System (RIS). (en)">
					comment = <"Når Radiologi informasjons systemet tildeler en identifikator, åpnes det for sporing av fremdriften/prosessforløpet på bestillingen av undersøkelsen/rekvisisjonen og for å koble undersøkelsesresultatet til rekvisisjonen/bestillingen. Som regel tilsvarer dette HL7 \"Filler Order Identifier\". Denne identifikatoren kalles også ofte \"rekvisisjonsnummer\" eller \"henvisningsnummer\".">
				>
				["at0033"] = <
					text = <"RapportID">
					description = <"*The unique identifier designated by the imaging service to the report. (en)">
				>
				["at0041"] = <
					text = <"Strukturert teknikk/prosedyre">
					description = <"Ytterligere strukturerte detaljer med tekniske eller kliniske aspekter knyttet til bildetakingen.">
				>
				["at0044"] = <
					text = <"Strukturerte funn">
					description = <"*Structured details about the imaging examination findings targeting specific structure or region. (en)">
				>
				["at0045"] = <
					text = <"Resultatrepresentasjon">
					description = <"Digitalt bilde eller video som representerer undersøkelsesresultatet.">
					comment = <"Flere formater tillates, men innholdet i de forskjellige formatene må representere det samme innholdet. For eksempel en pdf versjon av rapporten.">
				>
				["at0046"] = <
					text = <"Tilleggsinformasjon">
					description = <"Ytterligere informasjon som trengs for å kunne registrere lokalt definert innhold eller for å tilpasse til andre referansemodeller/formalismer.">
					comment = <"For eksempel lokale informasjonsbehov eller ytterligere metadata for å kunne tilpasse til tilsvarende konsepter i FHIR.">
				>
				["at0047"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0048"] = <
					text = <"Konfunderende faktorer">
					description = <"Tekstlig beskrivelse av faktorer som kan påvirke undersøkelsesfunn og/eller tolkning, og som ikke er registrert annet sted.">
				>
				["at0049"] = <
					text = <"Teknikkbeskrivelse">
					description = <"Tekstlig beskrivelse om kliniske aspekter knyttet til bildetakingen.">
					comment = <"*Use this data element to provide an outline of technique, including technical limitations or deviation from the normal technique. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data. (en)">
				>
				["at0055"] = <
					text = <"*Target body site (en)">
					description = <"Enkel beskrivelse av sted eller region på eller i kroppen som er målet for undersøkelsen.">
					comment = <"Hvis målområde er spesifisert av en kode i undersøkelsesnavnet, er dette feltet ikke nødvendig. Vanligvis ekvivalent til FHIR ImagingStudy.series.bodySite.">
				>
				["at0056"] = <
					text = <"Sammenligning">
					description = <"Tekstlig beskrivelse med sammenligning mellom denne undersøkelsen og tidligere lignende undersøkelser.">
					comment = <"Bruk også dette dataelementet for å indikere om ingen sammenligningsbilder og/eller rapporter er tilgjengelige.">
				>
				["at0057"] = <
					text = <"*Imaging quality description (en)">
					description = <"Tekstlig beskrivelse av kvaliteten på undersøkelsen.">
					comment = <"For eksempel om det er begrensninger ved undersøkelsen og deres konsekvens for tolkning, anbefaling om å gjenta undersøkelsen eller om alternative undersøkelsesmetoder som kan være mer formålstjenlig.">
				>
				["at0058"] = <
					text = <"Bildediagnostisk differensialdiagnose">
					description = <"Enkeltord, setning eller kort beskrivelse som uttrykker en mulig tilstand eller diagnose.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn en differensialdiagnose. Det er anbefalt å bruke en terminologi dersom mulig. Dataelementet bør være gjensidig utelukkende for \"Bildediagnostisk diagnose\", bare ENTEN \"Bildediagnostisk differensialdiagnose\" ELLER \"Bildediagnostisk diagnose\" bør forekomme i hvert Bildediagnostisk svar.">
				>
				["at0059"] = <
					text = <"Anbefaling">
					description = <"Forslag til videre bildediagnostiske undersøkelser, granskninger og/eller henvisninger, inkludert begrunnelser knyttet til dette.">
					comment = <"Dette dataelementet er repeterbart for å tillate flere enn en anbefaling. Bruk en INSTRUCTION-arketype for formelle henvisninger for ytterligere bildeundersøkelser, for eksempel INSTRUCTION.service_request.">
				>
				["at0061"] = <
					text = <"Problemstilling">
					description = <"Tekstlig beskrivelse av relevante kliniske opplysninger som gir kontekst for undersøkelsen og tolkning av funn.">
					comment = <"Dette kan inkludere en lenke til klinisk informasjon som er gitt i den originale henvisningen eller annet sted i den elektroniske pasientjournalen.">
				>
				["at0062"] = <
					text = <"Prosedyrebeskrivelse">
					description = <"Tekstlig beskrivelse om kliniske aspekter knyttet til bildetakingen.">
					comment = <"*Use this data element to describe a summary of the clinical procedure, patient positioning, interventions such as Valsalva manoeuvre, and administration of contrast agent, radiopharmaceuticals and/or treatments as part of the imaging study. For example: '80 ml of Isovue-370 was administered intravenously using a bolus-tracking technique centered on the main pulmonary artery'. It can also be used to record an overview of any issues, complications or adverse reactions encountered.  (en)">
				>
				["at0063"] = <
					text = <"*Imaging quality (en)">
					description = <"Vurdering om kvaliteten på undersøkelsen.">
					comment = <"Det anbefales å kode undersøkelseskvalitet med et verdi- eller kodesett, for eksempel \"Utmerket\", \"God\", \"Bra\", \"Dårlig\" og \"Ikke diagnostiserbar\", eller \"Adekvat\", \"Suboptimal\" og \"Inadekvat\".">
				>
				["at0065"] = <
					text = <"*Study details (en)">
					description = <"Detaljer om hvert bilde som er besvart.">
				>
				["at0067"] = <
					text = <"Sammenligningsbilde/-r">
					description = <"*Details about an image being compared to the reported image/s. (en)">
				>
				["at0069"] = <
					text = <"Utstyr">
					description = <"">
				>
				["at0070"] = <
					text = <"Undersøkelsesdato">
					description = <"*The date and time or interval of data and time when the imaging examination was carried out. (en)">
				>
				["at0071"] = <
					text = <"Tidsangivelse for overordnet resultatstatus">
					description = <"*The date and/or time that ‘Overall test status’ was assigned. (en)">
				>
				["at0072"] = <
					text = <"Overordnet resultatstatus">
					description = <"Den overordnede statusen for hele svaret.">
					comment = <"Verdiene er valgt spesifikt for å samsvare med verdiene i HL7 FHIR-ressursen \"Diagnostic report\", som historisk sett kommer fra HL7 v2. Andre lokale koder eller termer kan brukes ved å bruke datatypen \"Fri eller kodet tekst.

Dette elementet kan repeteres for å understøtte bruksområdene der statuser for forskjellige aspekter av resultatet er delt opp i flere elementer. Et eksempel på dette er de norske standard labsvarmeldingene i HIS 1.4-standarden, der status for svaret og status for laboratorieprosessen håndteres separat.">
				>
				["at0073"] = <
					text = <"Registrert">
					description = <"*The existence of the test is registered in the radiology source system, but no result is yet available. (en)">
				>
				["at0074"] = <
					text = <"Ufullstendig">
					description = <"*This is a partial (e.g. initial, interim or preliminary) result but data may be incomplete or unverified. (en)">
				>
				["at0075"] = <
					text = <"Foreløpig">
					description = <"Verifiserte tidlige svar er tilgjengelige, men ikke alle svar er endelige. Dette er en underkategori av \"Ufullstendig\".">
				>
				["at0076"] = <
					text = <"Endelig">
					description = <"*The result is complete and verified by an authorised person. (en)">
				>
				["at0077"] = <
					text = <"Revidert">
					description = <"*Subsequent to being final, the result has been modified. (en)">
				>
				["at0078"] = <
					text = <"Korrigert">
					description = <"*Subsequent to being final, the result has been modified to correct an error. This is a sub-category of 'Amended'. (en)">
				>
				["at0079"] = <
					text = <"Tillegg">
					description = <"*Subsequent to being final, the result has been modified by adding new content. The existing content is unchanged. This is a sub-category of 'Amended'. (en)">
				>
				["at0080"] = <
					text = <"Kansellert">
					description = <"Svaret er utilgjengelig fordi analysen ikke ble påbegynt eller ferdigstilt (også kalt \"avbrutt\").">
				>
				["at0081"] = <
					text = <"Feilregistrert">
					description = <"*The result has been withdrawn following previously been issued as final. (en)">
				>
				["at0083"] = <
					text = <"Kopimottakere">
					description = <"Detaljert informasjon knyttet til kliniker eller organisatorisk enhet som skal motta en kopi av radiologirapporten.">
					comment = <"Personene eller organisasjonene på distribusjonslisten mottar svaret kun til informasjon, og det er hovedmottakeren av rapporten som forventes å agere på den.">
				>
				["at0087"] = <
					text = <"Teknikk">
					description = <"Navn på teknikken eller den tekniske prosedyren som brukes for å ta bildet/bildene.">
					comment = <"*Coding of the 'Technique' with a terminology is pemitted. Multiple occurrences allow for identification of one or more techniques. If the technique needs to be recorded at study level, use the 'Technique' data element within the CLUSTER.imaging_study_detail archetype. (en)">
				>
				["at0088"] = <
					text = <"Prosedyre">
					description = <"Identifisering av den kliniske prosedyren eller håndteringen som brukes som en del av å bildetakningen.">
					comment = <"*In implementations, RM links can be created from this data element to relevant ACTION.medication, ACTION.procedure or similar archetypes stored within the health record that document the actual clinical procedures and related activities carried out. If the procedure needs to be recorded at study level, use the 'Procedure' data element within the CLUSTER.imaging_study_details archetype instead. (en)">
				>
				["at0090"] = <
					text = <"*Unknown (en)">
					description = <"*The status of the result is not known. (en)">
				>
			>
		>
		["de"] = <
			items = <
				["at0000"] = <
					text = <"Bildgebendes Untersuchungsergebnis">
					description = <"Zur Darstellung der Befunde und der Interpretationen einer durchgeführten bildgebenden Untersuchung.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Beliebiges Ereignis">
					description = <"Standardwert, ein undefinierter/s Zeitpunkt oder Intervallereignis, das explizit im Template oder zur Laufzeit der Anwendung definiert werden kann.">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Name der Untersuchung">
					description = <"Der Name der/des durchgeführten bildgebenden Untersuchung oder Prozedur.">
					comment = <"Es wird dringend empfohlen, mit einer Terminologie zu codieren, vorzugsweise mit einem Begriff, der sowohl die Modalität als auch die anatomische Position angibt, sofern möglich. Mögliche Kandidatenterminologien sind LOINC, SNOMED CT oder RadLex.">
				>
				["at0006"] = <
					text = <"*Structured target body site (en)">
					description = <"Strukturierte Angaben über die anatomische Stelle in Bezug auf das gesamte Ergebnis.">
					comment = <"Wenn das \"Zielgebiet\" durch einen Code im \"Name der Untersuchung\" angegeben wird, kann dieses Datenelement überflüssig sein. Die Identifizierung einer anatomischen Stelle in Bezug auf einen bestimmten Befund wird in jeder Instanz des CLUSTER.imaging_finding dargestellt.">
				>
				["at0008"] = <
					text = <"Bildgebende Befunde">
					description = <"Die Beschreibung der bildgebenden Befunde.">
				>
				["at0014"] = <
					text = <"*Clinical information provided (en)">
					description = <"*Narrative description of relevant clinical history that provides context for the examination and interpretation of results. Also known as 'Clinical summary'. (en)">
					comment = <"Dies kann einen Link zu den klinischen Informationen enthalten, die in der ursprünglichen Untersuchungsanfrage oder an anderer Stelle der Gesundheitsakte angegeben wurden.">
				>
				["at0020"] = <
					text = <"Bildgebende Diagnose">
					description = <"Eine einzelne Wortangabe, eine Phrase oder eine kurze Beschreibung, die einen wahrscheinlichen Zustand oder eine wahrscheinliche Diagnose darstellt.">
					comment = <"Dieses Datenelement kann mehrfach vorkommen, um mehr als eine Diagnoseangabe zu ermöglichen. Wenn möglich, wird die Codierung mit einer Terminologie bevorzugt. Dieses Datenelement sollte als sich gegenseitig ausschließend für die \"Differentialdiagnosen\" angesehen werden - in jedem bildgebenden Untersuchungsergebnis sollte nur eine \"Differentialdiagnose\" ODER \"Bildgebende Diagnose\" vorhanden sein.">
				>
				["at0021"] = <
					text = <"Schlussfolgerung">
					description = <"Kurz beschriebene, klinisch relevante Interpretation aller bildgebenden Befunde und gegebenenfalls ein Vergleich zu früheren Studien.">
					comment = <"*Also referred to as 'Opinion' or 'Conclusion'. Usually equivalent to the FHIR DiagnosticReport.conclusion. (en)">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Zusätzliche Informationen über die Untersuchung, welche noch nicht an anderer Stelle dargestellt wurden.">
					comment = <"Zum Beispiel: eine Notiz, dass der Film dem Patienten gegeben wurde.">
				>
				["at0024"] = <
					text = <"Zeitstempel der Ergebnisausgabe">
					description = <"*The date and/or time that the result was issued for the recorded 'Overall test status'. (en)">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"Bildgebungsdienst">
					description = <"Demografische Angaben über den Dienst, die Organisation oder die Person, die die bildgebende Untersuchung durchführt.">
				>
				["at0027"] = <
					text = <"Details zum Untersuchungsauftrag">
					description = <"Details zu einem einzelnen Untersuchungsauftrag.">
					comment = <"Normalerweise gibt es für jedes Ergebnis einen Untersuchungsauftrag, jedoch können unter Umständen mehrere Untersuchungsanforderungen mit einem einzigen Archetyp \"Bildgebendes Untersuchungsergebnis\" dargestellt werden.">
				>
				["at0028"] = <
					text = <"Auftragskennung des Auftragsgebers">
					description = <"*The local identifier assigned to the order by the order requester. (en)">
					comment = <"*Equivalent to the HL7 Placer Order Identifier. (en)">
				>
				["at0029"] = <
					text = <"Name der angeforderter Untersuchung">
					description = <"Identifizierung der angeforderten bildgebenden Untersuchung oder der angeforderten Prozedur, wenn die angeforderte Untersuchung von der tatsächlich durchgeführten Untersuchung abweicht.">
				>
				["at0030"] = <
					text = <"Auftragsgeber">
					description = <"Details über den Kliniker oder die Organisation, der/die die bildgebende Untersuchung angefordert hat.">
				>
				["at0031"] = <
					text = <"*Receiver order identifier (en)">
					description = <"*The local identifier assigned to the imaging examination order by the order filler, usually by the receiving Radiology Information System (RIS). (en)">
					comment = <"*Assigning an identifier to a request by the Radiology lnformation System (RIS) enables tracking progress of the request and enables linking results to requests. It also provides a reference to assist with enquiries and it is usually equivalent to the HL7 Filler Order Identifier. (en)">
				>
				["at0033"] = <
					text = <"Berichtskennung">
					description = <"*The unique identifier designated by the imaging service to the report. (en)">
				>
				["at0041"] = <
					text = <"Technik / Prozedur strukturiert">
					description = <"Zusätzliche strukturierte Informationen zu technischen und klinischen Aspekten der Aufnahme des Bildes/der Bilder.">
				>
				["at0044"] = <
					text = <"*Structured imaging findings (en)">
					description = <"*Structured details about the imaging examination findings targeting specific structure or region. (en)">
				>
				["at0045"] = <
					text = <"Berichtsrepräsentation">
					description = <"*Digital representation of the examination result. (en)">
					comment = <"*Multiple formats are allowed but they should represent equivalent clinical content. For example a pdf version of the report.  (en)">
				>
				["at0046"] = <
					text = <"Erweiterung">
					description = <"Zusätzliche Informationen zur Erfassung lokaler Inhalte oder Anpassung an andere Referenzmodelle/Formalismen.">
					comment = <"Zum Beispiel: Lokaler Informationsbedarf oder zusätzliche Metadaten zur Anpassung an FHIR-Ressourcen.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Störfaktoren">
					description = <"Die Beschreibung von Faktoren, welche noch nicht an anderer Stelle erfasst werden, die Untersuchungsbefunde und / oder -ergebnisse beeinflussen können.">
				>
				["at0049"] = <
					text = <"*Technique description (en)">
					description = <"*Narrative description about the technique or technical procedure used to capture the image/s. (en)">
					comment = <"*Use this data element to provide an outline of technique, including technical limitations or deviation from the normal technique. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data. (en)">
				>
				["at0055"] = <
					text = <"*Target body site (en)">
					description = <"Einfache Beschreibung der Körperstelle oder -region, die für die Bildgebung bestimmt ist.">
					comment = <"*If the 'Target site' is specified by a code in the 'Test name', then this field may be redundant. Usually equivalent to the FHIR ImagingStudy.series.bodySite (en)">
				>
				["at0056"] = <
					text = <"Vergleich">
					description = <"Die Beschreibung zum Vergleich dieser Untersuchung mit früheren ähnlichen Studien.">
					comment = <"*Also use this data element to indicate if no comparison images and/or reports are available.  (en)">
				>
				["at0057"] = <
					text = <"*Imaging quality description (en)">
					description = <"*Narrative description about the quality of the examination. (en)">
					comment = <"Zum Beispiel: die Art jeglicher Einschränkungen und deren Auswirkung auf die Interpretation, Empfehlung zur Wiederholung der Studie oder ob eine alternative Beurteilungsmethode geeigneter sein könnte.">
				>
				["at0058"] = <
					text = <"Bildgebende Differentialdiagnose">
					description = <"Eine einzelne Wortangabe, eine Phrase oder eine kurze Beschreibung, die einen möglichen Zustand oder eine mögliche Diagnose darstellt.">
					comment = <"Dieses Datenelement kann mehrfach vorkommen, um mehr als eine Angabe der Differentialdiagnose zu ermöglichen. Wenn möglich, wird die Codierung mit einer Terminologie bevorzugt. Dieses Datenelement sollte als sich gegenseitig ausschließend für die \"Bildgebende Diagnose\" angesehen werden - in jedem bildgebenden Untersuchungsergebnis sollte nur eine \"Differentialdiagnose\" ODER \"Bildgebende Diagnose\" vorhanden sein.">
				>
				["at0059"] = <
					text = <"Empfehlung">
					description = <"Vorschlag für weitere Bildgebung, Untersuchungen und/oder Überweisung und dazugehörige Begründung.">
					comment = <"Dieses Datenelement hat die Kardinalität 0..*, um mehr als eine Empfehlung zu ermöglichen. Formelle Anordnungen für zusätzliche bildgebende Untersuchungen, Prüfungen sollten unter Verwendung eines INSTRUCTION-Archetyps wie INSTRUCTION.service_request dargestellt werden.">
				>
				["at0061"] = <
					text = <"*Reason for request (en)">
					description = <"*Narrative description about the reason for requesting the examination. Also known as 'Clinical query'. (en)">
				>
				["at0062"] = <
					text = <"*Procedure description (en)">
					description = <"*Narrative description about the clinical procedure or other clinical considerations used to capture the image/s. (en)">
					comment = <"*Use this data element to describe a summary of the clinical procedure, patient positioning, interventions such as Valsalva manoeuvre, and administration of contrast agent, radiopharmaceuticals and/or treatments as part of the imaging study. For example: '80 ml of Isovue-370 was administered intravenously using a bolus-tracking technique centered on the main pulmonary artery'. It can also be used to record an overview of any issues, complications or adverse reactions encountered.  (en)">
				>
				["at0063"] = <
					text = <"*Imaging quality (en)">
					description = <"Die Beurteilung der Qualität der Untersuchung.">
					comment = <"Es wird empfohlen, die Qualität der Bildgebung mit einer Wertemenge zu codieren. Zum Beispiel: „ausgezeichnet, gut, mittelmäßig, schlecht und nicht diagnostisch“ oder „angemessen, suboptimal und unzureichend“.">
				>
				["at0065"] = <
					text = <"*Study details (en)">
					description = <"Die Informationen zu jeder berichteten Bildaufnahme.">
				>
				["at0067"] = <
					text = <"Vergleichsaufnahme/-n">
					description = <"*Details about an image being compared to the reported image/s. (en)">
				>
				["at0069"] = <
					text = <"Gerät">
					description = <"">
				>
				["at0070"] = <
					text = <"*Examination date (en)">
					description = <"*The date and time or interval of data and time when the imaging examination was carried out. (en)">
				>
				["at0071"] = <
					text = <"Zeitstempel des gesamten Teststatus">
					description = <"*The date and/or time that ‘Overall test status’ was assigned. (en)">
				>
				["at0072"] = <
					text = <"Gesamtteststatus">
					description = <"*The status of the imaging examination result as a whole. (en)">
					comment = <"Die Werte wurden so ausgewählt, dass sie mit denen im HL7 FHIR Diagnosebericht übereinstimmen, der historisch aus der HL7v2Praxis stammt. Andere lokale Codes/Begriffe können über den Text \"Auswahl\" verwendet werden. 

Dieses Element kann mehrfach vorkommen, um Fälle abzudecken, bei denen der Status für verschiedene Aspekte des Ergebnisses in mehrere Elemente unterteilt wurde.
">
				>
				["at0073"] = <
					text = <"Registriert">
					description = <"*The existence of the test is registered in the radiology source system, but no result is yet available. (en)">
				>
				["at0074"] = <
					text = <"Teilweise">
					description = <"*This is a partial (e.g. initial, interim or preliminary) result but data may be incomplete or unverified. (en)">
				>
				["at0075"] = <
					text = <"Vorläufig">
					description = <"Es sind erste, bestätigte Ergebnisse verfügbar, aber nicht alle Ergebnisse sind final. Dies ist eine Unterkategorie von \"Teilweise\".">
				>
				["at0076"] = <
					text = <"Final">
					description = <"*The result is complete and verified by an authorised person. (en)">
				>
				["at0077"] = <
					text = <"Abgeändert">
					description = <"*Subsequent to being final, the result has been modified. (en)">
				>
				["at0078"] = <
					text = <"Korrigiert">
					description = <"*Subsequent to being final, the result has been modified to correct an error. This is a sub-category of 'Amended'. (en)">
				>
				["at0079"] = <
					text = <"Hinzugefügt">
					description = <"*Subsequent to being final, the result has been modified by adding new content. The existing content is unchanged. This is a sub-category of 'Amended'. (en)">
				>
				["at0080"] = <
					text = <"Abgebrochen">
					description = <"Das Ergebnis ist nicht verfügbar, weil der Test nicht gestartet oder nicht abgeschlossen wurde (manchmal auch als \"gescheitert\" bezeichnet).">
				>
				["at0081"] = <
					text = <"Irrtümlich eingegeben">
					description = <"*The result has been withdrawn following previously been issued as final. (en)">
				>
				["at0083"] = <
					text = <"Verteilerliste">
					description = <"Details über weitere Kliniker oder Organisationen, die eine Kopie der Analyseergebnisse benötigen.">
					comment = <"Die \"Verteilerliste\" dient nur zu Informationszwecken. Der Hauptempfänger des Berichts ist die Person, die dazu bestimmt ist, auf die Information zu reagieren.">
				>
				["at0087"] = <
					text = <"*Technique (en)">
					description = <"*Name of the technique or technical procedure used to capture the image/s. (en)">
					comment = <"*Coding of the 'Technique' with a terminology is pemitted. Multiple occurrences allow for identification of one or more techniques. If the technique needs to be recorded at study level, use the 'Technique' data element within the CLUSTER.imaging_study_detail archetype. (en)">
				>
				["at0088"] = <
					text = <"*Procedure (en)">
					description = <"*Identification of the clinical procedure or management used as part of capturing the image/s. (en)">
					comment = <"*In implementations, RM links can be created from this data element to relevant ACTION.medication, ACTION.procedure or similar archetypes stored within the health record that document the actual clinical procedures and related activities carried out. If the procedure needs to be recorded at study level, use the 'Procedure' data element within the CLUSTER.imaging_study_details archetype instead. (en)">
				>
				["at0090"] = <
					text = <"*Unknown (en)">
					description = <"*The status of the result is not known. (en)">
				>
			>
		>
		["sv"] = <
			items = <
				["at0000"] = <
					text = <"Bilddiagnostiskt resultat">
					description = <"Registrering av resultat och tolkning av en bilddiagnostisk undersökning eller en serie av undersökningar.">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Ospecificerad händelse">
					description = <"Förinställd, ospecificerad tidpunkt eller tidsintervall som kan definieras mer uttryckligen i en mall (template) eller i en applikation.
">
				>
				["at0003"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0004"] = <
					text = <"Namn på bilddiagnostisk undersökning">
					description = <"Namnet på den bilddiagnostiska undersökningen eller proceduren som utförts.">
					comment = <"*Coding with a terminology is strongly recommended, preferably a term specifying both modality and anatomical location, where possible. Possible candidate terminologies are LOINC, SNOMED CT or RadLex. (en)">
				>
				["at0006"] = <
					text = <"*Structured target body site (en)">
					description = <"Strukturerade detaljer om den anatomiska platsen i förhållande till hela resultatet.


">
					comment = <"*If the 'Target site' is specified by a code in the 'Test name', then this field may be redundant. Identification of an anatomical site related to a specific finding will be recorded within each instance of CLUSTER.imaging_finding. (en)">
				>
				["at0008"] = <
					text = <"*Imaging findings (en)">
					description = <"Fritextbeskrivning av de kliniska fynden.">
				>
				["at0014"] = <
					text = <"*Clinical information provided (en)">
					description = <"*Narrative description of relevant clinical history that provides context for the examination and interpretation of results. Also known as 'Clinical summary'. (en)">
					comment = <"*This may include a link to the clinical information provided in the original examination request or elsewhere in the health record. (en)">
				>
				["at0020"] = <
					text = <"Diagnos">
					description = <"Enstaka ord, fras eller kort beskrivning som representerar det troliga tillståndet eller diagnosen.">
					comment = <"Detta dataelement kan förekomma mer än en gång för att tillåta fler än en diagnos. Kodning med en terminologi är att föredra där det är möjligt. Detta dataelement bör betraktas som ömsesidigt uteslutande avseende \"Differentialdiagnos\" - endast en av \"Diagnos\" ELLER \"Differentialdiagnos\" bör finnas i varje bildundersökningsresultat.">
				>
				["at0021"] = <
					text = <"Slutsats">
					description = <"Kortfattad och kliniskt relevant tolkning av alla bilddiagnostiska fynd, inklusive en jämförelse med tidigare studier där så är lämpligt.
">
					comment = <"*Also referred to as 'Opinion' or 'Conclusion'. Usually equivalent to the FHIR DiagnosticReport.conclusion. (en)">
				>
				["at0023"] = <
					text = <"Kommentar">
					description = <"Ytterligare beskrivning av undersökningen som inte fångats i andra fält.">
					comment = <"Till exempel: en anteckning om att filmen gavs till patienten.">
				>
				["at0024"] = <
					text = <"Datum/tid för utfärdat resultat">
					description = <"*The date and/or time that the result was issued for the recorded 'Overall test status'. (en)">
				>
				["at0025"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0026"] = <
					text = <"*Imaging service (en)">
					description = <"*Demographic details about the service, organisation or individual carrying out the imaging examination. (en)">
				>
				["at0027"] = <
					text = <"Beställningsdetaljer">
					description = <"*Details concerning a single examination requested.  (en)">
					comment = <"*Usually there is one examination request for each result, however in some circumstances multiple examination requests may be represented using a single Imaging examination result archetype. (en)">
				>
				["at0028"] = <
					text = <"Beställnings-ID">
					description = <"*The local identifier assigned to the order by the order requester. (en)">
					comment = <"*Equivalent to the HL7 Placer Order Identifier. (en)">
				>
				["at0029"] = <
					text = <"Namn på beställd undersökning">
					description = <"Identifiering av bildundersökning eller begärd procedur, där den begärda undersökningen skiljer sig från den faktiska utförda undersökningen.

">
				>
				["at0030"] = <
					text = <"Beställare">
					description = <"Detaljer om den läkare eller organisation som beställt den bilddiagnostiska undersökningen.">
				>
				["at0031"] = <
					text = <"*Receiver order identifier (en)">
					description = <"*The local identifier assigned to the imaging examination order by the order filler, usually by the receiving Radiology Information System (RIS). (en)">
					comment = <"*Assigning an identifier to a request by the Radiology lnformation System (RIS) enables tracking progress of the request and enables linking results to requests. It also provides a reference to assist with enquiries and it is usually equivalent to the HL7 Filler Order Identifier. (en)">
				>
				["at0033"] = <
					text = <"Rapport-ID">
					description = <"*The unique identifier designated by the imaging service to the report. (en)">
				>
				["at0041"] = <
					text = <"*Structured technique/procedure (en)">
					description = <"*Additional structured details of technical and clinical aspects of capturing the image/s. (en)">
				>
				["at0044"] = <
					text = <"*Structured imaging findings (en)">
					description = <"*Structured details about the imaging examination findings targeting specific structure or region. (en)">
				>
				["at0045"] = <
					text = <"*Report representation (en)">
					description = <"*Digital representation of the examination result. (en)">
					comment = <"*Multiple formats are allowed but they should represent equivalent clinical content. For example a pdf version of the report.  (en)">
				>
				["at0046"] = <
					text = <"Tilläggsinformation">
					description = <"Ytterligare information som krävs för att utvidga modellen med lokalt innehåll eller för anpassning till andra referensmodeller.
">
					comment = <"Till exempel: lokala informationskrav eller ytterligare metadata för anpassning till FHIR.">
				>
				["at0047"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0048"] = <
					text = <"Möjliga felkällor">
					description = <"Beskrivning av faktorer som inte registrerats någon annanstans, men som kan påverka fynden och/eller undersökningsresultatet.">
				>
				["at0049"] = <
					text = <"*Technique description (en)">
					description = <"*Narrative description about the technique or technical procedure used to capture the image/s. (en)">
					comment = <"*Use this data element to provide an outline of technique, including technical limitations or deviation from the normal technique. For example: '5 mm axial images from the lung bases through the pubic symphysis were acquired following the administration of intravenous and oral contrast. Coronal and Sagittal reformatted images were constructed from the source data. (en)">
				>
				["at0055"] = <
					text = <"*Target body site (en)">
					description = <"*Simple description about the body site or region targetted for imaging. (en)">
					comment = <"*If the 'Target site' is specified by a code in the 'Test name', then this field may be redundant. Usually equivalent to the FHIR ImagingStudy.series.bodySite (en)">
				>
				["at0056"] = <
					text = <"*Comparison (en)">
					description = <"*Narrative description about the comparison of this examination, with previous similar studies. (en)">
					comment = <"*Also use this data element to indicate if no comparison images and/or reports are available.  (en)">
				>
				["at0057"] = <
					text = <"*Imaging quality description (en)">
					description = <"*Narrative description about the quality of the examination. (en)">
					comment = <"*For example: the nature of any limitations and their impact on interpretation; recommendation that the examination should be repeated; or whether an alternative method of assessment might be more appropriate. (en)">
				>
				["at0058"] = <
					text = <"Differentialdiagnos">
					description = <"Enstaka ord, fras eller kort beskrivning som representerar ett möjligt tillstånd eller en diagnos.">
					comment = <"Detta dataelement kan förekomma mer än en gång för att tillåta fler än en differentialdiagnos. Kodning med en terminologi är att föredra där det är möjligt. Detta dataelement bör betraktas som ömsesidigt uteslutande avseende \"Diagnos\" - endast en av \"Differentialdiagnos\" ELLER \"Diagnos\" bör finnas i varje bildundersökningsresultat.

">
				>
				["at0059"] = <
					text = <"Rekommendation">
					description = <"Förslag för ytterligare avbildning, undersökningar och/eller hänvisning och tillhörande motiv.

">
					comment = <"*This data element has 0..* occurrences to allow for more than one recommendation. Formal orders for additional imaging examination, investigation should be recorded using an INSTRUCTION archetype, such as INSTRUCTION.service_request. (en)">
				>
				["at0061"] = <
					text = <"*Reason for request (en)">
					description = <"*Narrative description about the reason for requesting the examination. Also known as 'Clinical query'. (en)">
				>
				["at0062"] = <
					text = <"*Procedure description (en)">
					description = <"*Narrative description about the clinical procedure or other clinical considerations used to capture the image/s. (en)">
					comment = <"*Use this data element to describe a summary of the clinical procedure, patient positioning, interventions such as Valsalva manoeuvre, and administration of contrast agent, radiopharmaceuticals and/or treatments as part of the imaging study. For example: '80 ml of Isovue-370 was administered intravenously using a bolus-tracking technique centered on the main pulmonary artery'. It can also be used to record an overview of any issues, complications or adverse reactions encountered.  (en)">
				>
				["at0063"] = <
					text = <"*Imaging quality (en)">
					description = <"*Assessment about the quality of the examination. (en)">
					comment = <"*Coding of the imaging quality with a value set is recommended. For example: 'excellent, good, fair, poor, and non-diagnostic'; or 'adequate, suboptimal and inadequate'.
 (en)">
				>
				["at0065"] = <
					text = <"*Study details (en)">
					description = <"*Details about each image being reported. (en)">
				>
				["at0067"] = <
					text = <"*Comparison image/s (en)">
					description = <"*Details about an image being compared to the reported image/s. (en)">
				>
				["at0069"] = <
					text = <"*Device (en)">
					description = <"">
				>
				["at0070"] = <
					text = <"*Examination date (en)">
					description = <"*The date and time or interval of data and time when the imaging examination was carried out. (en)">
				>
				["at0071"] = <
					text = <"Overall test status timestamp">
					description = <"*The date and/or time that ‘Overall test status’ was assigned. (en)">
				>
				["at0072"] = <
					text = <"Overall test status">
					description = <"*The status of the imaging examination result as a whole. (en)">
					comment = <"The values have been specifically chosen to match those in the HL7 FHIR Diagnostic report, historically derived from HL7v2 practice. Other local codes/terms can be used via the Text 'choice'.

This element is multiple occurrence to cater for the use cases where statuses for different aspects of the result have been split into several elements.">
				>
				["at0073"] = <
					text = <"Registered">
					description = <"*The existence of the test is registered in the radiology source system, but no result is yet available. (en)">
				>
				["at0074"] = <
					text = <"Partial">
					description = <"*This is a partial (e.g. initial, interim or preliminary) result but data may be incomplete or unverified. (en)">
				>
				["at0075"] = <
					text = <"Preliminary">
					description = <"Verified early results are available, but not all results are final. This is a sub-category of 'Partial'.">
				>
				["at0076"] = <
					text = <"Final">
					description = <"*The result is complete and verified by an authorised person. (en)">
				>
				["at0077"] = <
					text = <"Amended">
					description = <"*Subsequent to being final, the result has been modified. (en)">
				>
				["at0078"] = <
					text = <"Corrected">
					description = <"*Subsequent to being final, the result has been modified to correct an error. This is a sub-category of 'Amended'. (en)">
				>
				["at0079"] = <
					text = <"Appended">
					description = <"*Subsequent to being final, the result has been modified by adding new content. The existing content is unchanged. This is a sub-category of 'Amended'. (en)">
				>
				["at0080"] = <
					text = <"Cancelled">
					description = <"The result is unavailable because the test was not started or not completed (also sometimes called 'aborted').">
				>
				["at0081"] = <
					text = <"Entered in error">
					description = <"*The result has been withdrawn following previously been issued as final. (en)">
				>
				["at0083"] = <
					text = <"Distribution list">
					description = <"Details of additional clinicians or organisations who require a copy of the test result.">
					comment = <"The 'Distribution list' is for information-only, and that the primary recipient of the report is the person intended to act on the information.">
				>
				["at0087"] = <
					text = <"*Technique (en)">
					description = <"*Name of the technique or technical procedure used to capture the image/s. (en)">
					comment = <"*Coding of the 'Technique' with a terminology is pemitted. Multiple occurrences allow for identification of one or more techniques. If the technique needs to be recorded at study level, use the 'Technique' data element within the CLUSTER.imaging_study_detail archetype. (en)">
				>
				["at0088"] = <
					text = <"*Procedure (en)">
					description = <"*Identification of the clinical procedure or management used as part of capturing the image/s. (en)">
					comment = <"*In implementations, RM links can be created from this data element to relevant ACTION.medication, ACTION.procedure or similar archetypes stored within the health record that document the actual clinical procedures and related activities carried out. If the procedure needs to be recorded at study level, use the 'Procedure' data element within the CLUSTER.imaging_study_details archetype instead. (en)">
				>
				["at0090"] = <
					text = <"*Unknown (en)">
					description = <"*The status of the result is not known. (en)">
				>
			>
		>
	>
