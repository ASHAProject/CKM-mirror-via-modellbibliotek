	details = <
		["nb"] = <
			language = <[ISO_639-1::nb]>
			purpose = <"For å registrere beregnet eller kjent gestasjonsalder på et bestemt tidspunkt for et fosteranlegg, embryo, foster eller en nyfødt.">
			use = <"Bruk for å registrere beregnet eller kjent gestasjonsalder på et bestemt tidspunkt for et fosteranlegg, embryo, foster eller en nyfødt.">
			keywords = <"gestasjon", "gestasjonsalder", "konsepsjonsalder", "modenhet">
			misuse = <"Skal ikke brukes for å registrere gestasjonsalder til et foster ved fødsel. Dette skal registreres i Graviditetssammenfatning.">
		>
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the estimated or known gestation age of a current pregnancy or new born at a point in time.">
			use = <"To record the estimated or known gestational age of a pregnancy or new born.">
			keywords = <"gestation", "gestational age", "conceptional age", "maturity">
			misuse = <"Not to record the historical gestational age of the foetus at birth. This is recorded in the pregnancy summary.">
