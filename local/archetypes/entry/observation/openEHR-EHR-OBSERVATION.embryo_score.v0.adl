definition
	OBSERVATION[at0000] matches {    -- Embryo score
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0010] occurrences matches {0..1} matches {    -- Label
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0011] occurrences matches {0..1} matches {    -- ID
										value matches {
											DV_IDENTIFIER matches {*}
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {    -- Description
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {    -- Cell number/stage
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0037,    -- Pronuclear stage
													at0038,    -- 2-cell stage
													at0039,    -- 4-cell stage
													at0040,    -- 8-cell stage
													at0041,    -- Morula
													at0042]    -- Blastocyst
												}
											}
										}
									}
									allow_archetype CLUSTER[at0013] occurrences matches {0..*} matches {    -- Findings
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.exam_embryo\.v0|openEHR-EHR-CLUSTER\.exam_blastocyst\.v0|openEHR-EHR-CLUSTER\.exam_oocyte\.v0|openEHR-EHR-CLUSTER\.exam_zygote\.v0/}
									}
									allow_archetype CLUSTER[at0022] occurrences matches {0..*} matches {    -- Image/video representation
									}
									ELEMENT[at0023] occurrences matches {0..1} matches {    -- Time post-insemination
										value matches {
											DV_DURATION matches {*}
										}
									}
									ELEMENT[at0024] occurrences matches {0..1} matches {    -- Interpretation
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0025,    -- Viable
													at0026]    -- Non-viable
												}
											}
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {    -- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
					POINT_EVENT[at0027] occurrences matches {0..1} matches {    -- Fertilisation check
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0028] occurrences matches {0..1} matches {    -- Syngamy check
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0029] occurrences matches {0..1} matches {    -- Early cleavage check
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0030] occurrences matches {0..1} matches {    -- Day-2 embryo assessment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0031] occurrences matches {0..1} matches {    -- Day-3 embryo assessment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0032] occurrences matches {0..1} matches {    -- Day-4 embryo assessment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
					POINT_EVENT[at0033] occurrences matches {0..1} matches {    -- Day-5 embryo assessment
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0004] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Device
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {    -- Container ID
						value matches {
							DV_IDENTIFIER matches {*}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Method
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0008,    -- Manual
									at0009]    -- Automatic
								}
							}
						}
					}
				}
			}
		}
	}


