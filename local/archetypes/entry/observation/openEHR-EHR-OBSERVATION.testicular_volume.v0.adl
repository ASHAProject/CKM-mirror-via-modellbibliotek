	openEHR-EHR-OBSERVATION.testicular_volume.v0

concept
	[at0000]	-- Testicular volume
language
	original_language = <[ISO_639-1::en]>
description
	original_author = <
		["name"] = <"John Tore Valand">
		["organisation"] = <"Helse Bergen University Hospital">
		["email"] = <"john.tore.valand@helse-bergen.no">
		["date"] = <"2017-04-26">
	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record the volume of an individual's testicles.">
			use = <"Use to record the volume of an individual's testicles.

The volume can be determined by comparing the testicles with the beads of an orchidometer, by measuring the length and with with a ruler or with sliding calipers, comparative palpation with ellipsoid models of known volume, or by ultrasound.">
			keywords = <"testis", ...>
			misuse = <"">
			copyright = <"© Nasjonal IKT HF, openEHR Foundation">
		>
	>
