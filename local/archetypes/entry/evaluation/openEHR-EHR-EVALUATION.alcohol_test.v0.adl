definition
	EVALUATION[at0000] matches {    -- Alcohol test
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Do you drink?
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Yes
									at0004]    -- No
								}
							}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Type of alcohol
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0006,    -- Beer
									at0007,    -- Wine
									at0008]    -- Spirits
								}
							}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {    -- Duration of drinking
						value matches {
							DV_DURATION matches {*}
						}
					}
					ELEMENT[at0010] occurrences matches {0..1} matches {    -- Contenxt
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0011,    -- Social
									at0012,    -- Alone
									at0013]    -- Occasional
								}
							}
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {    -- Frequency
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0015,    -- Once a day
									at0016,    -- Once a week
									at0017,    -- Weekends
									at0018]    -- With/without dinner
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0019] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0020] occurrences matches {0..1} matches {    -- Date last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}


