definition
	EVALUATION[at0000] matches {	-- Eligible for DR screening
		data matches {
			ITEM_TREE[at0001] matches {	-- Arbol
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Admittance in screening
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0008, 	-- Eligible for screening
									at0009, 	-- Ineligible for screening 
									at0010]	-- Excluded from screening
								}
							}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Exclusion statement
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0011, 	-- Excluded from screening as blind
									at0012, 	-- Excluded from screening as deceased
									at0013, 	-- Excluded from screening as learning disability
									at0014, 	-- Excluded from screening as moved away
									at0015, 	-- Excluded from screening as no current contact details
									at0016, 	-- Excluded from screening as no longer diabetic
									at0017, 	-- Excluded from screening as physical disorder
									at0018, 	-- Excluded from screening as terminal illness
									at0019, 	-- Excluded from screening as under care of ophthalmologist
									at0020]	-- Other
								}
							}
						}
					}
					ELEMENT[at0021] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0005] matches {	-- Arbol
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Date last decision
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}


