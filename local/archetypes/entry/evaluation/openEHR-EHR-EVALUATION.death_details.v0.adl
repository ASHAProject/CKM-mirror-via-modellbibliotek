definition
	EVALUATION[at0000] matches {    -- Death details
		data matches {
			ITEM_TREE[at0002] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0018] occurrences matches {0..1} matches {    -- Deceased
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0019,    -- Yes
									at0020]    -- No
								}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Main cause of death
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {    -- Time of death
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {    -- Place of death
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0006] occurrences matches {0..*} matches {    -- Chain of events
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0007] matches {    -- Contributory cause
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0008] matches {    -- Causation order
								value matches {
									DV_TEXT matches {*}
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0009,    -- Contributory cause B
											at0010,    -- Contributory cause C
											at0011]    -- Contributory cause D
										}
									}
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {    -- Duration of cause
								value matches {
									DV_TEXT matches {*}
									DV_INTERVAL<DV_DURATION> matches {
										lower matches {
											DV_DURATION matches {
												value matches {|>=PT0S|}
											}
										}
									}
									DV_DURATION matches {
										value matches {|>=PT0S|}
									}
								}
							}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {    -- Comorbidity
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0014] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0016] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0017] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


