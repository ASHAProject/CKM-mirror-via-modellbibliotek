definition
	EVALUATION[at0000] matches {	-- Adverse Reaction (AllergyIntolerance)
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {3..*; unordered} matches {
					ELEMENT[at0002] matches {	-- Substance/Agent
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0094] occurrences matches {0..1} matches {	-- Substance/Agent Category
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0063] matches {	-- Status**
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0064, 	-- Suspected
									at0065, 	-- Confirmed
									at0066, 	-- Refuted
									at0067]	-- Resolved
								}
							}
						}
					}
					ELEMENT[at0053] occurrences matches {0..1} matches {	-- Criticality (FHIR)
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0054, 	-- Fatal
									at0055, 	-- High
									at0056, 	-- Medium
									at0057]	-- Low
								}
							}
						}
					}
					ELEMENT[at0101] occurrences matches {0..1} matches {	-- Criticality (DAM)**
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0102, 	-- Low risk
									at0103]	-- High risk
								}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Absolute Contraindication?**
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0058] occurrences matches {0..1} matches {	-- SensitivityType**
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0059, 	-- Allergy / Immune-mediated reaction
									at0060]	-- Intolerance / non-Immune mediated reaction
								}
							}
						}
						null_flavour existence matches {0..1} matches {
							DV_CODED_TEXT matches {
								defining_code matches {[openehr::253]}
							}
						}
					}
					CLUSTER[at0009] occurrences matches {0..*} matches {	-- Adverse Reaction Event
						items cardinality matches {2..*; unordered} matches {
							ELEMENT[at0087] occurrences matches {0..1} matches {	-- Did Not Occur Flag**
								value matches {
									DV_BOOLEAN matches {
										value matches {True}
									}
								}
							}
							CLUSTER[at0111] occurrences matches {0..*} matches {	-- Manifestation details
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0011] matches {	-- Manifestation**
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {[local::at0105]}		-- No reaction
											}
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0110] occurrences matches {0..1} matches {	-- Severity of Manifestation **
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {	-- Reaction Description
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0016] occurrences matches {0..1} matches {	-- Reaction Type**
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0059, 	-- Allergy / Immune-mediated reaction
											at0060]	-- Intolerance / non-Immune mediated reaction
										}
									}
								}
								null_flavour existence matches {0..1} matches {
									DV_CODED_TEXT matches {
										defining_code matches {[openehr::253]}
									}
								}
							}
							ELEMENT[at0112] occurrences matches {0..1} matches {	-- Certainty**
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0113, 	-- Suspected
											at0114, 	-- Probable
											at0115]	-- Confirmed
										}
									}
								}
							}
							ELEMENT[at0027] occurrences matches {0..1} matches {	-- Onset of Reaction
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {	-- Duration of Reaction
								value matches {
									DV_DURATION matches {*}
								}
							}
							ELEMENT[at0089] occurrences matches {0..1} matches {	-- Severity of Reaction**
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0090, 	-- Severe
											at0091, 	-- Serious
											at0092, 	-- Moderate
											at0093]	-- Minor
										}
									}
								}
							}
							ELEMENT[at0032] occurrences matches {0..1} matches {	-- Reaction Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0029] occurrences matches {0..*} matches {	-- Additional Reaction Detail
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0031] occurrences matches {0..*} matches {	-- Multimedia
								value matches {
									DV_MULTIMEDIA matches {
										media_type matches {[openEHR::]}
									}
								}
							}
							CLUSTER[at0100] occurrences matches {0..*} matches {	-- Exposure Details
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0010] matches {	-- Specific Substance/Agent
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0106] occurrences matches {0..1} matches {	-- Exposure Type**
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0107, 	-- Drug administration
													at0108, 	-- Immunisation
													at0109]	-- Accidental
												}
											}
										}
									}
									ELEMENT[at0021] occurrences matches {0..1} matches {	-- Causality Expectation**
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0095, 	-- Unlikely
													at0023, 	-- Likely
													at0024]	-- Confirmed
												}
											}
										}
										null_flavour existence matches {0..1} matches {
											DV_CODED_TEXT matches {
												defining_code matches {[openehr::253]}
											}
										}
									}
									ELEMENT[at0020] occurrences matches {0..1} matches {	-- Earliest Exposure
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0025] occurrences matches {0..1} matches {	-- Duration of Exposure
										value matches {
											DV_DURATION matches {*}
										}
									}
									ELEMENT[at0018] occurrences matches {0..1} matches {	-- Exposure Description
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0096] occurrences matches {0..*} matches {	-- Further Exposure Details
										include
											archetype_id/value matches {/.*/}
									}
								}
							}
							ELEMENT[at0040] occurrences matches {0..1} matches {	-- Clinical Management Description
								value matches {
									DV_TEXT matches {*}
								}
							}
							allow_archetype CLUSTER[at0041] occurrences matches {0..*} matches {	-- Reporting Details
								include
									archetype_id/value matches {/.*/}
							}
							CLUSTER[at0097] occurrences matches {0..1} matches {	-- FHIR AdverseReaction record provenance
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0072] occurrences matches {0..*} matches {	-- Identifier
										value matches {
