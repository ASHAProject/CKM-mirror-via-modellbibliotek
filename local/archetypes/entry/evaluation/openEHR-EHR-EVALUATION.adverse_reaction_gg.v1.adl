										value matches {
											DV_URI matches {*}
										}
									}
								}
							}
						}
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {	-- Future Use**
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0006] occurrences matches {0..1} matches {	-- Overall Comment
