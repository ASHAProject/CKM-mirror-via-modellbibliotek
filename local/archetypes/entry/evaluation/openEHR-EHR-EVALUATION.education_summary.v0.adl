definition
	EVALUATION[at0000] matches {    -- Education and training summary
		data matches {
			ITEM_TREE[at0001] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0018] occurrences matches {0..1} matches {    -- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {    -- Age Commenced Formal Education
						value matches {
							DV_DURATION matches {
								value matches {PYM/|>=PT0S|}
							}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {    -- Highest Achieved
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype CLUSTER[at0029] occurrences matches {0..*} matches {    -- Education episode
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0030] occurrences matches {0..*} matches {    -- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {    -- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0026] matches {    -- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0027] occurrences matches {0..*} matches {    -- Extension
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0028] occurrences matches {0..1} matches {    -- Last updated
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Education and training summary">
					description = <"Summary or persistent information about an individual's current and past education and training.">
				>
				["at0001"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Highest Achieved">
					description = <"Description of highest level of education or training achieved to date.">
					comment = <"Coding with a terminology is desirable, where possible. For example: completed secondary school; completed postgraduate degree; completed apprenticeship; or never attended school. ">
				>
				["at0003"] = <
					text = <"Age Commenced Formal Education">
					description = <"Age when the individual commenced formal education.">
				>
				["at0007"] = <
					text = <"Comment">
					description = <"Additional narrative about the individual's education, not captured in other fields.">
				>
				["at0018"] = <
					text = <"Description">
					description = <"Narrative description about the entire education and training history of the individual.">
				>
				["at0026"] = <
					text = <"Tree">
					description = <"@ internal @">
				>
				["at0027"] = <
					text = <"Extension">
					description = <"Additional information required to capture local content or to align with other reference models/formalisms.">
					comment = <"For example: local information requirements or additional metadata to align with FHIR or CIMI equivalents.">
				>
				["at0028"] = <
					text = <"Last updated">
					description = <"The date that this information was last updated.">
					comment = <"At implementation, it is assumed that if an associated education record is added or updated then this date will also be updated.">
				>
				["at0029"] = <
					text = <"Education episode">
					description = <"Structured details about each education or training, both current and past.">
				>
				["at0030"] = <
					text = <"Additional details">
					description = <"Additional details about the education or training, or previous education history of an individual.">
				>
			>
		>
	>
